//Write a C program to find sum of the elements in an array.
#include <stdio.h>
int main()
{
    int n;
    int sum=0;
    printf("The size of the array:");
    scanf("%d",&n);
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        printf("Enter element in the index %d:",i);
        scanf("%d",&arr[i]);
    }
    for (int i = 0; i < n; i++)
    {
        sum+=arr[i];
    }
    printf("The sum of the elements in the array is:%d",sum);
}
